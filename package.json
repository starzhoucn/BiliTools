{
  "name": "catlair-bilitools",
  "version": "0.3.20",
  "description": "this is BiliTools",
  "main": "index.ts",
  "scripts": {
    "build": "babel ./src --out-dir ./dist --extensions \".ts\"",
    "build:ql": "rollup -c qinglong/rollup.config.ts",
    "deploy": "sls deploy --debug",
    "deploy:multi": "node tools/bootstrap.js --scf",
    "start:multi": "node tools/bootstrap.js --start",
    "dev": "ts-node ./src/run.ts",
    "start": "node ./dist/run.js",
    "liveHeart": "node ./dist/runLiveHeart.js",
    "modclean": "modclean -r -a \"*.md,*LICENSE*,.github,.nycrc,*.ts,*.html,*.txt,*.js.map,*.mjs.map,browser.js,@types\" && node tools/pkgclean.js",
    "prebuild": "rm -rf dist/",
    "predeploy:multi": "rm -f ./dist/run.js && cp -rf ./tools/ ./package*.json ./dist && cd ./dist && npm i --production && npm run modclean && rm -rf tools package*.json &&  cd ../",
    "postbuild": "cd ./dist && rm -rf ./dto/ ./interface/ *.d.js",
    "lint:eslint": "eslint --max-warnings 0  \"**/*.{js,ts}\" --fix",
    "lint:prettier": "prettier --write  \"**/*.{js,json,ts,md}\""
  },
  "author": "catlair",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@types/crypto-js": "^4.1.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.10.3",
    "@types/nodemailer": "^6.4.4",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "crypto-js": "^4.1.1",
    "got": "^11.8.2",
    "lodash": "^4.17.21",
    "nodemailer": "^6.7.2",
    "pako": "1.0.11",
    "qs": "^6.10.2",
    "tencentcloud-sdk-nodejs": "^4.0.268",
    "tough-cookie": "^4.0.0",
    "tunnel": "^0.0.6"
  }
}
